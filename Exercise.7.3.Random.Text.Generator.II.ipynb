    "with open(\"HelloPython.txt\", \"r\") as my_file:\n",
    "    d = my_file.read()\n",
    "    w = d.split() \n",
    "    word_posisition = random.randint(0, len(w)-1)\n",
    "    x=w[word_posisition].replace(\"Python\", \"Java\")\n",
    "    print(\"Random words\", w[word_posisition])\n",
    "    print(\"Change : \",x)"
   ]
